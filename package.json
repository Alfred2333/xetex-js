{
  "name": "xetex",
  "version": "0.0.0",
  "description": "JavaScript port of XeTeX",
  "author": "David Xu",
  "keywords": [
    "latex",
    "pdf",
    "tex",
    "xdvipdfmx",
    "xelatex",
    "xetex"
  ],
  "homepage": "https://www.github.come/lyze/xetex-js",
  "license": "MIT",
  "main": "xetex.js",
  "browser": "workercontroller.umd.js",
  "jsnext:main": "workercontroller.js",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/lyze/xetex-js.git"
  },
  "scripts": {
    "rollup": "rollup -c",
    "dist": "make && npm run rollup",
    "clean": "rm -f workercontroller.umd.js workercontroller.umd.js.map workercontroller.umd.min.js workercontroller.umd.min.js.map",
    "doc": "jsdoc -d doc -c .jsdocconf.json -a all workercontroller.js ",
    "lint": "eslint --ext .js,.html .",
    "test-node-raw": "tape -r ./test/babel test/node/**/*.js",
    "test-node": "tape -r ./test/babel test/node/**/*.js | tap-spec",
    "test-browser": "zuul --coverage -- test/browser/**/*.js",
    "test-browser-local": "zuul --local 8080 --coverage -- test/browser/**/*.js",
    "test-local": "npm run test-node && npm run test-browser-local",
    "test": "npm run dist && npm run test-node && npm run test-browser",
    "start": "npm run rollup && http-server .",
    "prepublish": "npm run dist"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-register": "^6.9.0",
    "babelify": "^7.3.0",
    "blue-tape": "^0.2.0",
    "browserify": "^13.0.1",
    "es6-promise": "^3.2.1",
    "eslint": "^2.11.1",
    "eslint-config-google": "^0.5.0",
    "eslint-plugin-html": "^1.5.1",
    "http-server": "^0.9.0",
    "jsdoc": "^3.4.0",
    "phantomjs-prebuilt": "^2.1.7",
    "rollup": "^0.28.0",
    "rollup-plugin-babel": "^2.5.1",
    "rollup-plugin-buble": "^0.10.0",
    "rollup-plugin-commonjs": "^3.0.0",
    "rollup-plugin-node-resolve": "^1.7.0",
    "rollup-plugin-uglify": "^0.3.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "zuul": "^3.12.0"
  },
  "private": true
}
