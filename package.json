{
  "name": "xetex",
  "version": "0.0.0",
  "description": "JavaScript port of XeTeX",
  "author": "David Xu",
  "keywords": [
    "xetex",
    "tex",
    "pdf"
  ],
  "homepage": "https://www.github.come/lyze/xetex-js",
  "license": "MIT",
  "main": "xetex.js",
  "browser": "xetexcontroller.umd.js",
  "jsnext:main": "xetexcontroller.js",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/lyze/xetex-js.git"
  },
  "scripts": {
    "rollup": "rollup -c",
    "dist": "npm run rollup",
    "clean": "rm -f xetexcontroller.umd.js xetexcontroller.umd.js.map xetexcontroller.umd.min.js xetexcontroller.umd.min.js.map",
    "start": "npm run rollup && http-server .",
    "lint": "eslint --ext .js,.html .",
    "raw-test": "babel-tape-runner test/**.js",
    "test": "babel-tape-runner test/**.js | tap-spec",
    "prepublish": "npm run dist"
  },
  "devDependencies": {
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-tape-runner": "^2.0.1",
    "eslint": "^2.11.1",
    "eslint-config-google": "^0.5.0",
    "eslint-plugin-html": "^1.5.1",
    "faucet": "0.0.1",
    "http-server": "^0.9.0",
    "rollup": "^0.28.0",
    "rollup-plugin-buble": "^0.10.0",
    "rollup-plugin-uglify": "^0.3.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1"
  },
  "private": true
}
