{
  "name": "xetex",
  "version": "0.0.0",
  "description": "JavaScript port of XeTeX",
  "author": "David Xu",
  "keywords": [
    "xetex",
    "tex",
    "pdf"
  ],
  "homepage": "https://www.github.come/lyze/xetex-js",
  "license": "MIT",
  "main": "xetex.umd.js",
  "browser": "xetex.umd.js",
  "jsnext:main": "xetex.js",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/lyze/xetex-js.git"
  },
  "scripts": {
    "rollup": "rollup xetex.js -n xetex -f umd -m xetex.umd.js.map -o xetex.umd.js",
    "dist": "npm run-script rollup && uglifyjs xetex.umd.js -c -m --in-source-map xetex.umd.js.map --source-map xetex.umd.min.js.map -o xetex.umd.min.js",
    "start": "npm run-script rollup && http-server .",
    "lint": "eslint xetex.js example/index.html",
    "prepublish": "npm run-script dist"
  },
  "devDependencies": {
    "eslint": "^2.11.1",
    "eslint-config-google": "^0.5.0",
    "eslint-plugin-html": "^1.5.1",
    "http-server": "^0.9.0",
    "rollup": "^0.26.3",
    "uglifyjs": "^2.4.10"
  },
  "private": true
}
